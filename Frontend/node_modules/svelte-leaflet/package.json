{
  "name": "svelte-leaflet",
  "version": "0.2.0",
  "description": "LeafletJS maps for the SvelteJS reactive framework",
  "main": "src/index.js",
  "author": "Iván Sánchez Ortega",
  "license": "LGPL-3.0",
  "private": false,
  "scripts": {
    "build-demo": "rollup -c demo/rollup.config.js",
    "serve-demo": "rollup -c demo/rollup.config.js -w",
    "pretty": "prettier --plugin-search-dir=. src/**.js src/**.svelte src/**/**.svelte demo/**.html demo/**.svelte demo/main.js --write"
  },
  "prettier": {
    "printWidth": 85,
    "tabWidth": 4,
    "useTabs": true,
    "trailingComma": "es5",
    "arrowParens": "always"
