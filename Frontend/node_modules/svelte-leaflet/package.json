    "rollup": "^2.18.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-svelte": "^5.2.3",
    "svelte": "^3.23.2"
  },
  "dependencies": {
    "leaflet": "^1.6.0"
  }
